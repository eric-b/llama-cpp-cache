{
  "urls": "http://localhost:8081",
  "Upstream": {
    "Url": "http://localhost:8080",
    "Timeout": "01:00:00"
  },
  "OutputCache": {
    "CacheDuration": "1.12:00:00"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "dd/MM HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
